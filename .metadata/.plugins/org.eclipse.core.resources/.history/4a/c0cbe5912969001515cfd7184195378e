package com.calculator;

class Primitives {

	public static void main(String [] args) {
		
//		byte
		byte one = 27;
		System.out.println("byte one = " + one + ".");
//		short
		short two = 14;
		System.out.println("short two = " + two + ".");
//		int
		int three = 26;
		System.out.println("int one = " + three + ".");
//		long
//		float
//		double
//		char
//		boolean
		
	}

}
