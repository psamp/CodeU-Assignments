package com.calculator;

class Primitives {

	public static void main(String [] args) {
		
//		byte
		byte one = 27;
		System.out.println("byte one = " + one + ".");
//		short
		short two = 14;
		System.out.println("short two = " + two + ".");
//		int
		int three = 26;
		System.out.println("int one = " + three + ".");
//		long
		long four = 178;
		System.out.println("long four = " + four + ".");
//		float
		float five = 78.89787676756563435359878767676673;
		System.out.println("float five = " + five + ".");
//		double
		double six = 3.14;
		System.out.println("double six = " + six + ".");
//		char
		char x = 88;
		System.out.println("char x = " + x + ".");
//		boolean
		boolean tf = true;
		System.out.println("boolean tf = " + tf + ".");
		
	}

}
