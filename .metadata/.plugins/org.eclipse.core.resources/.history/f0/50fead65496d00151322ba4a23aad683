package com.psamp.stocks;

class StockClient {

	public static void main(String[] args) {
		
//		New array of Stocks class.
		Stock[][] stocks = {
				{new Stock("ABC", 20.01, 1.30)},
				{new Stock("DEF", 30.11, .99)},
				{new Stock("DEF", 30.11, .99)},
				{new Stock("JKL", 10.31, 1.19)},
				{new Stock("MNO", 23.55, 4.21)}
		};
		
		for (int i = 0; i < stocks.length; i++) {
			System.out.println(stocks[i]);
			for (int j = 0; j < stocks[i].length; j++) {
				System.out.println(stocks[i][i.stockName]);
			}
		}
	}
}
