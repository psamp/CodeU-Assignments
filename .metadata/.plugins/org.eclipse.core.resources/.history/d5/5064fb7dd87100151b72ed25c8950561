package com.psamp.rental;

public class RentalCar {
	Long id;
	String make;
	String model;
	
	RentalCar(String make, String model) {
		this.make = make;
		this.model = model;
	}
	
	int getRoomNumber() {
		return this.roomNumber;
	}
	
	void setRoomNumber(int roomNumber) {
		this.roomNumber = roomNumber;
	}
	
	boolean getSmoking() {
		return this.smoking;
	}
	
	void setSmoking(boolean smoking) {
		this.smoking = smoking;
	}

}

}
