package com.psamp.day4;

//Create Box class. Add methods that set the dimensions and compute and print the volume.
 class Box {
	 double height;
	 double width;
	 double depth;
	 
	 double volume(String varName) {
		 double vol;
		 vol = width * height * depth;
		 System.out.println("The volume of " + varName + " is " + vol);
		 return 0;
	 }

 
	 void setDim(double w, double h, double d) {
		 width = w;
		 height = h;
		 depth = d;
	 }
 }
	
 class BoxDemo5 {

	public static void main(String[] args) {
		
		Box myBox1 = new Box(myBox1);
		Box myBox2 = new Box(myBox2);
		
		myBox1.setDim(10,20,15);
		myBox2.setDim(3,6,9);
		
		myBox1.volume();
		myBox2.volume();
		
	}

}
