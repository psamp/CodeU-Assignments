package com.psamp.printstars;
import java.util.Arrays;

class PrintStars {

	
	public static void main(String[] args) {
		
		int oneThroughTwenty[] = {1, 2, 3, 4, 5};
		
		for (int i = 0; i < oneThroughTwenty.length; i++) {
			System.out.println(oneThroughTwenty[i]);
//			for (int j = 0; j < i + 1; j++) {
//				System.out.print("*");
//			}
		}

	}

}