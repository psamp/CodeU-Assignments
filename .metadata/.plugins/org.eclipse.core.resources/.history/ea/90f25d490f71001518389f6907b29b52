package com.psamp.lifecycletest;

import static org.junit.Assert.*;

import org.junit.Test;

import junit.framework.TestSuite;

public class JUnitLifeCycleTestLoop {

	@Test
	public void test() {
		
		TestSuite suite = new TestSuite(JUnitLifeCycleTestLoop.class.getName());
		for (int i = 0; i < 30; i++) {
			suite.addTest(JUnitLifecycleTest.class);
		}
		
	}

}
