package com.psamp.lang;

import static org.junit.Assert.*;

import org.junit.Test;

public class StringTest {

	@Test
	public void testLength() {
		String s = "JUnit Rules";
		assertEquals(11, s.length());
		
	}
	
	@Test
	public void testSub() {
		String s = "JUnit Rules";
		assertEquals(true, s.substring(1, 4));
	}

}
