
class CharDemo {

	public static void main(String[] args) {
		
//	Create eight new characters, and set their values to unicode hex ids.
	char one, two, three, four, five, six, seven, eight;
		one = \u\0050;
		two = 83;
		three = 74;
		four = 79;
		five = 68;
		six = 70;
		seven = 84;
		eight = 84;
		
		System.out.print("Hello, my name is" + one + two + three + four + five + six + seven + eight);
		
	}

}
