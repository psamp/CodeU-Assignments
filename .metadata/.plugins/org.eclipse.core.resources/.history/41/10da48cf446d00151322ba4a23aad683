package com.psamp.stocks;

class StockClient {

	public static void main(String[] args) {
		
//		New array of Stocks class.
		Stock[][] stocks = new Stock[5][3];
		
		for (int i = 0; i < stocks.length; i++) {
			
			for (int j = 0; j < i; j++) {
				System.out.println(stocks[i][j]);
			}
			
		}

	}

}
