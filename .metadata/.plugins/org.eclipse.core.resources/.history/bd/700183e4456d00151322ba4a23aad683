package com.psamp.stocks;

class StockClient {

	public static void main(String[] args) {
		
//		New array of Stocks class.
		Stock[] stocks = new Stock[5];
		
		stocks[0] = new Stock("ABC", 20.01, 1.30);
		stocks[1] = new Stock("DEF", 30.11, .99);
		stocks[2] = new Stock("GHI", 41.23, 1.99);
		stocks[3] = new Stock("JKL", 10.31, 1.19);
		stocks[4] = new Stock("MNO", 23.55, 4.21);

	}

}
