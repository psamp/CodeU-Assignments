package com.codeu.event.rental;

public class CreditCard {
	private String firstName;
	private String lastName;
	private String experationDate;
	private int securityCode;
	private long cardNumber;
	
	CreditCard() {
		
	}
	
	CreditCard() {
		
	}

	public String getFirstName() {
		return firstName;
	}

	public String getLastName() {
		return lastName;
	}

	public String getExperationDate() {
		return experationDate;
	}

	public int getSecurityCode() {
		return securityCode;
	}

	public long getCardNumber() {
		return cardNumber;
	}

	private void setFirstName(String firstName) {
		this.firstName = firstName;
	}

	private void setLastName(String lastName) {
		this.lastName = lastName;
	}

	private void setExperationDate(String experationDate) {
		this.experationDate = experationDate;
	}

	private void setSecurityCode(int securityCode) {
		this.securityCode = securityCode;
	}

	private void setCardNumber(long cardNumber) {
		this.cardNumber = cardNumber;
	}
	
	

}
