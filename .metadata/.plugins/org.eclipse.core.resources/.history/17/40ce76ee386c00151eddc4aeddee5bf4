package com.psamp.fixstring;

public class FixString {

	public static void main(String[] args) {
		String a = "Jovo is      ";
		String b = "  a great langu";
		
//		To uppercase.
		String upperB = b.toUpperCase();
		String upperA = a.toUpperCase();

		//		Fix.
		String completeLanguage = upperB + "AGE";
		
		System.out.println(completeLanguage);
	}

}
