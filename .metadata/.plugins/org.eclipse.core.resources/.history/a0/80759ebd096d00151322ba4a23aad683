package com.psamp.mssg;
import java.util.Scanner;

public class DailyMessageClient {

//	Start program.
	public static void main(String[] args) {
		
//		New instance of DailyMEssage.
		DailyMessage dm = new DailyMessage();
		
//		Call dailyMessage and pass in different days with various cases.
		dm.getMessage("FRIday");
		dm.getMessage("SATURDAY");
		dm.getMessage("monday");
		dm.getMessage("Humpday");

	}

}
