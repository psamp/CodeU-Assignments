package com.psamp.lifecycletest;

import static org.junit.Assert.*;

import org.junit.Test;

public class JUnitLifeCycleTestLoop {

	@Test
	public void test() {
		
		TestSuite suite = new TestSuite(RepeatTest.class.getName());
		for (int i = 0; i < 30; i++) {
			suite.addTestSuite(JUnitLifecycleTest.class);
		}
		
	}

}
