package com.psamp.powerco;

class CorpControlRoom {
	private static Plant[] plants;
	
	static {
		plants = new Plant[5];
		
		plants[0] = new Electric(553, 9, 14.88);
		plants[1] = new Solar(118, 2, "Sunnyville");
		plants[2] = new Electric(667, 18, 2.30);
		plants[3] = new Solar(478, 1, "Lightly");
		plants[4] = new Electric(235, 5, 3.45);
		plants[5] = new Solar(768, 4, "Maui");
		
	}
	
	void toString() {
		String strng = "";
		for (int i = 0; i < plants.length; i++) {
			plants[i].fail();
			plants[i].generatePower();
			plants[i].chargeForService();
			plants[i].toString();
		}
	}

}
