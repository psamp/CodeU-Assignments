package com.psamp.stocks;

class Stock {
	String stockName = "";
	double stockPrice = 0;
	double stockEarnings = 0;
	double peRatio;
	
	Stock(String sn, double sp, double se) {
		this.stockName = sn;
		this.stockPrice = sp;
		this.stockEarnings = se;
	}
	
	double calcPERatio() {
		double per = this.stockPrice / stockEarnings;
		return per;
	}
}
