package com.psamp.util;

class RentalCar {
	String make;
	String model;
	long year;
	long ID;
	
	RentalCar(long y, String ma, String mo, long i) {
		this.year = y;
		this.make = ma;
		this.model = mo;
		this.ID = i;
	}
	
	RentalCar(String ma, String mo, long i) {
		this.make = ma;
		this.model = mo;
		this.year = 2009;
		this.ID = i;
	}
	
	@Override
	public String toString() {
		String mssg = this.year + " " + this.make + " " + this.model + " | ID: " + this.ID;
		return mssg;	
	}

	public String getMake() {
		return make;
	}

	public String getModel() {
		return model;
	}

	public long getYear() {
		return year;
	}

	public long getID() {
		return ID;
	}

	public void setMake(String make) {
		this.make = make;
	}

	public void setModel(String model) {
		this.model = model;
	}

	public void setYear(long year) {
		this.year = year;
	}

	public void setID(long iD) {
		ID = iD;
	}
	
	
}
