package com.psamp.contronbreak;

class ControlNBreak {

	public static void main(String[] args) {
		
		for (int i = 0; i < 11; i++) {
			
			switch(i) {
				case 3:
				case 6:
				case 9:
					System.out.println("We're multiples of three! " + i);
					continue;
				case 2:
				case 4:
				case 8:
					System.out.println("We're multiples of two! So is six but three has it!" + i);
					break;
				default:
					System.out.println("We're less than ten!");
			}
			
		}

	}

}
