package com.psamp.powerco;

class Solar extends Plant {
	
	@Override
	int generatePower() {
		System.out.println("Solar's generate power");
		return 700_000;
	}

	@Override
	boolean fail() {
		boolean failed = false;
		if(System.currentTimeMillis() % 3 == 0) {
			failed = true;
		}
		
		return failed;
	}

}
