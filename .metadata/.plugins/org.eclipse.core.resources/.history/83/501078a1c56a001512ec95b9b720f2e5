package com.psamp.box7usedefault;

class Box {
	  double width;
	  double height;
	  double depth;
	  
	  // Set Dimensions.
	  Box(double w, double h, double d) {
	    width = w;
	    height = h;
	    depth = d;
	}
	  //Return volume.
	  double volume(String name) {
	    double vol = width * height * depth;
	    System.out.println("The volume of " + name + " is " + vol + ".");
	  }
	}