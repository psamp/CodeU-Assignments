package com.psamp.stocks;
import java.util.Arrays;

class StockClient {

	public static void main(String[] args) {
		
//		New array of Stocks.
		Stock stocks[] = {
				new Stock("ABC", 20.01, 1.30),
				new Stock("DEF", 30.11, .99),
				new Stock("GHI", 30.11, 1.99),
				new Stock ("JKL", 0.31, 1.19),
				new Stock ("MNO", 23.55, 4.21)
		};
		
		Stock stocksPERatio[] = new Stock[5];
		
		for (int i = 0; i < stocksPERatio.length; i++) {
			stocks[i].stockName = stocksPERatio[i].stockName;
			stocks[i].peRatio = stocksPERatio[i].peRatio;
		}
		
		System.out.println(stocksPERatio[0].stockName + " | " + stocksPERatio[0].peRatio);
		
		
		
	}
}
