package com.psamp.box7usedefault;

class Box {
	  double width;
	  double height;
	  double depth;
	  
	  // Set Dimensions.
	  Box() {
	    width = 10;
	    height = 20;
	    depth = 30;
	}
	  //Return volume.
	  volume(String name) {
	    double vol = width * height * depth;
	    System.out.println("The volume of " + name + " is " + vol + ".");
	  }
	}