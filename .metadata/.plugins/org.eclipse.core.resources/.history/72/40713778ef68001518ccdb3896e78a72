package com.psamp.day4;

 class Box {
	 double height;
	 double width;
	 double depth;
	 
	 double volume() {
		 return width * height * depth;
	 }

 
	 void setDim(double w, double h, double d) {
		 width = w;
		 height = h;
		 depth = d;
	 }
 }
	
 class BoxDemo5 {

	public static void main(String[] args) {
		
		Box myBox1 = new Box();
		Box myBox2 = new Box();
		
		myBox1.setDim(10,20,15);
		mybox2.setDim(3,6,9);
		
	}

}
