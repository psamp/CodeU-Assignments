
class CharDemo {

	public static void main(String[] args) {
		
//	Create eight new characters, and set their values to unicode ids.
	char one, two, three, four, five, six, seven, eight;
		one = 0049;
		two = 0083;
		three = 0074;
		four = 0079;
		five = 0068;
		six = 0070;
		seven = 0084;
		eight = 0084;
		
		System.out.print("Hello, my name is" + one + two + three + four + five + six + seven + eight);
		
	}

}
