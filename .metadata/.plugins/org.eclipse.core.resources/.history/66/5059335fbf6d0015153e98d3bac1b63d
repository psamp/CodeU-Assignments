package com.psamp.average;

class Average {

	public static void main(String[] args) {
		
		String nums[] = {"98", "84", "59", "87", "71"};
		double avg = 0;
		
		for (int i = 0; i < nums.length; i++) {
			avg +=  Integer.parseDouble(nums[i]) / nums.length;
		}
		
		System.out.println("The class average is: " + avg);
	}

}