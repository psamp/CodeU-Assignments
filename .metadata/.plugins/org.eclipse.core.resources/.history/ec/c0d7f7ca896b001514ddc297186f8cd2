package com.psamp.calculator;

class Calculator {

	double addThese(double a, double b) {
		double sum = a + b;
		return sum;
	}
	
	double subtractThese(double x, double y) {
		double sum = x - y;
		return sum;
	}
	
	double divideThese(double j, double k) {
		double sum = j / k;
		return sum;
	}
	
}
