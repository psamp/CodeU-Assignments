package com.psamp.contronbreak;

class ControlNBreak {

	public static void main(String[] args) {
		
		for (int i = 0; i < 11; i++) {
			
			lessThan10: {
			
			if ( i <= 10) {
				System.out.println("We're less than or equal to 10!");
			}
		}
			
			switch(i) {
				case 3:
				case 6:
				case 9:
					System.out.println("We're multiples of three! " + i);
					break;
				case 10:
					continue lessThan10;
				case 2:
				case 4:
				case 8:
					System.out.println("We're multiples of two! So is six but three has it! " + i);
			}
			
		}

	}

}
