package com.pp.usesym;

public class USPop {
	
	static long popGrowth(int years) {
		int x;
		long pop;
		double rate;
		long growth;
		
		for(x = 0; x <= years; x++) {
			pop = 318_900_000;
			rate = .7 * years;
			
			growth = pop * rate;
			
		}
		return growth;
	}
	
	public static void main(String [] args) {
	
		int growth;
		growth = popGrowth(5)
		
	}
	
}
