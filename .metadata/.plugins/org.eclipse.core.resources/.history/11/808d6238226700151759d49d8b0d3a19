
class CharDemo {
//	Call main to start the program.
	public static void main(String[] args) {
//	Create eight new characters, and set their values to ASCII DEC that point to the desired letters and case.
	char p, r, i, n, c, e, s1, s2;
		p = 80;
		r = 114;
		i = 105;
		n = 110;
		c = 99;
		e = 101;
		s1 = 115;
		s2 = 115;
		
		System.out.print("Hello, my name is " + p + r + i + n + c + e + s1 + s2 + ".");
		
	}

}
