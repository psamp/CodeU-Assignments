package com.psamp.day4;
import java.util.Arrays;

class AdmireArray {

	public static void main(String[] args) {
		String admireArray[] = {"Rodney Sampson I", "Shanterria Sampson", "Bucky Barnes", "Avatar Korra", "Ahsoka Tano"};
		
		for(int x=0; x<admireArray.length; x++) {
			System.out.println("I admire " + Arrays.sort(admireArray[x]););	
		}

	}

}
