package com.psamp.printstars;
import java.util.Arrays;

class PrintStars {

	
	public static void main(String[] args) {
		
		double oneThroughTwenty[] = [1, 2, 3, 4, 5];
		
		for (int i = 0; i < 21; i++) {
			System.out.println("\n" + i);
			for (int j = 0; j < i ; j++) {
				System.out.print("*");
			}
		}

	}

}