package com.codeu.currency;

public abstract class Currency {
	char currencySymbol;
	
	
	public Currency(char currencySymbol) {
		super();
		this.setCurrencySymbol(currencySymbol);
	}

	public abstract String printReceipt(int amount);
	public abstract String printReceipt(CreditCard card);

	public char getCurrencySymbol() {
		return currencySymbol;
	}

	private void setCurrencySymbol(char currencySymbol) {
		this.currencySymbol = currencySymbol;
	}

	
	
	
}
