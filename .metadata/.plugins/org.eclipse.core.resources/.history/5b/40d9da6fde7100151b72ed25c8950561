package com.psamp.rental;

import java.util.Random;

public class RentalCar {
	private Long ID;
	private String make;
	private String model;
	
	RentalCar(String make, String model) {
		Random rndm = new Random();
		
		this.make = make;
		this.model = model;
		this.ID = rndm.nextLong();
	}
	
	String getMake() {
		return this.make;
		System.out.println("ID : " + this.ID);
	}
	
	String getModel() {
		return this.model;
	}
	
	void setMake(String make) {
		this.make = make;
	}
	
	void setModel(String model) {
		this.model = model;
	}
	
	Long getID() {
		return this.ID;
	}

}
