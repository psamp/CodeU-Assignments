package com.psamp.boxdemo7;

public class Box {
	
	double height;
	double width;
	double depth;
	
	double volume(String boxName) {
		 double vol;
		 vol = width * height * depth;
		 System.out.println("The volume of " + boxName + " is " + vol);
		 return 0;
	 }
	
	void setDim(double w, double h, double d) {
		 width = w;
		 height = h;
		 depth = d;
	 }
}
