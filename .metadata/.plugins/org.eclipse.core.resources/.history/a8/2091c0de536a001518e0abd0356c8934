package com.psamp.toswitch;

class IfToSwitch {

	public static void main(String[] args) {
		int month = 4;
		String season;
		
		switch(month) {
//		winter
		case 12:
			season = "It's Winter.";
		case 1:
			season = "It's Winter.";
		case 2:
			
//		spring
		case 3:
			season = "It's Spring.";
		case 4:
			season = "It's Spring.";
		case 5:
			season = "It's Spring.";
//		it's summer
		case 6:
			season = "It's Summer.";
		case 7:
			season = "It's Summer.";
		case 8:
			season = "It's Summer.";
//		fall
		case 9:
			season = "It's Fall.";
		case 10:
			season = "It's Fall.";
		case 11:
			season = "It's Fall.";
		}

	}

}